{
  // "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "experimentalDecorators": true,
    "lib": ["ESNext"],
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "ES2020",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./src/*", "./src/", "./utils/*", "./utils/", "./interfaces/*", "*"]
    }
  },
  "include": ["**/*.ts", "./src/**/*.ts", "./utils/**/*.ts"],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
  // "ts-node": {
  //   "require": ["tsconfig-paths/register"]
  // }
}
